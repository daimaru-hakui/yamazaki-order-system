// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  // url = "file./dev.db"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @unique
  email String @unique
  username String?
  role UserRole @default(USER)
  @@map("users")
}

enum UserRole {
  USER
  ADMIN
  MEMBER
}